#!/bin/sh

SSH_USER=git \
SSH_PRIVATE_KEY=$(heroku config:get SSH_PRIVATE_KEY) \
SSH_PUBLIC_KEY=$(heroku config:get SSH_PUBLIC_KEY) \
GITHUB_REPO_READ_TOKEN=$(heroku config:get GITHUB_REPO_READ_TOKEN) \
GITHUB_REPO_STATUS_WRITE_TOKEN=$(heroku config:get GITHUB_REPO_STATUS_WRITE_TOKEN) \
foreman start --port 1201
