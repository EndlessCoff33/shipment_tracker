<%= ENV.fetch('RAILS_ENV') %>:
  adapter: postgresql
  database: <%= ENV.fetch('SHIPMENT_TRACKER_DATABASE_NAME') %>
  host: <%= ENV.fetch('SHIPMENT_TRACKER_DATABASE_HOST') %>
  password: <%= ENV.fetch('SHIPMENT_TRACKER_DATABASE_PASSWORD') %>
  pool: 8
  timeout: 5000
  username: <%= ENV.fetch('SHIPMENT_TRACKER_DATABASE_USERNAME') %>
